    "demo": "cross-env REACT_APP_HEROKU_OVERGANG=true REACT_APP_MOCK_ENDRE_REG_LOP=true REACT_APP_MOCK=true REACT_APP_MOCK_MANUELL_REGISTRERING=true npm-run-all -p start:tekster start:ts",
